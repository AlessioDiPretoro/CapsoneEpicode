@model IEnumerable<Stones.Models.AuctionsDetails>

@{
    ViewBag.Title = "UserAuctions";
}

<h2>Le tue offerte per le aste</h2>

<table class="table">
    <tr>
        <th>
           Codice asta
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AuctionsProducts.Product.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.data)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.price)
        </th>
        <th>Aggiudicata</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.idAuction)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AuctionsProducts.Product.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.data)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.price)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AuctionsProducts.idWinner)
        </td>
        @*<td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>*@
    </tr>
}

</table>
